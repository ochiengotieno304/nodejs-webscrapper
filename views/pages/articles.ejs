<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head'); %>
</head>

<body class="container">

  <header>
    <%- include('../partials/header'); %>
  </header>

  <main>
    <div class="jumbotron">
      <h1>Scrappy</h1>
      <p>Scrappy says hi!</p>

      <ul>
        <% articles.forEach(function(article) { %>
          <li>
            <strong>
              <%= article.title %>
            </strong>
          </li>
          <% }); %>
      </ul>
    </div>
  </main>

  <footer>
    <%- include('../partials/footer'); %>
  </footer>

</body>

</html>